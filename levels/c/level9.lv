//快速排序
void quickSort(int s[], int l, int r) {
	if (l < r) {      
		int i = l, j = r, x = s[l];
		while (i < j) {
			// 从右向左找第一个小于x的数
			while (i < j && s[j] >= x)
				j--;
			if (i < j)
				s[i++] = s[j];
			// 从左向右找第一个大于等于x的数
			while (i < j && s[i] < x)
				i++;
			if (i < j)
				s[j--] = s[i];
		}
		s[i] = x;
		// 递归调用
		quickSort(s, l, i - 1);
		quickSort(s, i + 1, r);
	}
}
