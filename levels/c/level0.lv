#include "Sudoku.h"
#include <iostream>
#include <string>

using namespace std;

void Sudoku::init() {
	//denote that the number is checked and confirmed.
	confirm = -2;
	for (int i = 0; i < 9; i++) {
		for (int j = 0; j < 9; j++) {
			//511=0b111111111 means the candidate for a cell can be 1-9
			sudoku[i][j] = 511;
			//the current status of the cell.
			//0: empty
			//-1: given
			//-2: filled and checked
			stage[i][j] = 0;
		}
	}
}
